.clearFix {
  zoom: 1;
}
.clearFix:after {
  content: "";
  clear: both;
  height: 0;
  display: block;
}
.image {
  width: 100%;
  height: 100%;
}
.block {
  display: block;
}

.container {
  height: 100%;
  display: flex;
  background-color: #f2f2f2;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
